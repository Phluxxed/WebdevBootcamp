<% include ./partials/header %>

<h4>Index Page!</h4>

<% blogs.forEach ((blog) => { %>
    <div>
        <h2><%= blog.title %></h2>
        <img src="<%= blog.image %>" style="width: 80%">
        <span><%= blog.created %></span>
        <p><%= blog.body %></p>
    </div>
<% }); %>

<% include ./partials/footer %>